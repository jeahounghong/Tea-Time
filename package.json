{
  "name": "teetime",
  "version": "1.0.0",
  "description": "Tee-Time is a full stack web application where users can connect with other users to find local games happening around their area.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend-install": "npm install --prefix frontend",
